<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <link rel="icon" href="/images/logo.png" type="image/png" sizes="16x16">
        <title>Chilling Music</title>
        <meta name="author" content="Chilling Music">
        <meta name="description" content="Welcome to Chilling Music, The place where you can listen to music and relax and enjoy your time 24/7!"/>
        <link rel="apple-touch-icon" href="/images/logo.png" />
        <meta property="og:title" content="Chilling Music" />
        <meta property="og:type" content="website" />
        <meta property="og:url" content="https://chillingmusic.cf" />
        <meta property="og:image" content="https://chillingmusic.cf/images/logo.png" />
        <meta property="og:description" content="Welcome to Chilling Music, The place where you can listen to music and relax and enjoy your time 24/7!" />
        <meta name="theme-color" content="#7701F7">
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/semantic-ui@2.4.2/dist/semantic.min.css">
                <script src="/socket.io/socket.io.js"></script>

        <script src="https://cdnjs.cloudflare.com/ajax/libs/pace/1.2.3/pace.min.js"></script>
          <script src="https://unpkg.com/axios/dist/axios.min.js"></script>

        <link href="/css/pace-theme.css" rel="stylesheet" />
        <link rel="stylesheet" href="/css/main.css">
    </head>

    <body>
        <div class="mainHeader">
            <h1 class="ui center aligned inverted header" style="font-family: 'Balsamiq Sans', cursive;">Chilling Music</h1>
            <h2 class="ui center aligned inverted sub header" style="font-family: 'Balsamiq Sans', cursive;">Welcome to Chilling Music, The place where you can listen to music and relax and enjoy your time 24/7!</h2>
        </div>

        <div class="controlingArea">
            <h4 class="ui horizontal divider inverted header" style="font-family: 'Balsamiq Sans', cursive;"><i class="music icon"></i>&nbsp;&nbsp;Music Control&nbsp;&nbsp;<i class="music icon"></i></h4>
            <div class="controlButtons">
                <button id="playButton" onclick="start('/api/run-quiz')" class="ui blue labeled icon button" style="font-family: 'Balsamiq Sans', cursive;"><i class="play icon"></i>run app</button>
                <button onclick="start('/api/new-quest')" id="pauseButton" class="ui purple labeled icon button" style="font-family: 'Balsamiq Sans', cursive;"><i class="pause icon"></i>next question</button>
                <button onclick="start('/api/send-answer')" id="restartButton" class="ui green labeled icon button" style="font-family: 'Balsamiq Sans', cursive;"><i class="redo icon"></i>show answer</button>
                <button onclick="start('/api/send-Rewards')" id="loopButton" class="ui orange labeled icon button" style="font-family: 'Balsamiq Sans', cursive;"><i class="sync icon"></i>send rewards</button>
                <button onclick="start('/api/shut-down')" id="songsListButton" class="ui red labeled icon button" style="font-family: 'Balsamiq Sans', cursive;"><i class="list icon"></i>shut down</button>
                <button onclick="start('startClick')" id="todaySongButton" class="ui yellow labeled icon button" style="font-family: 'Balsamiq Sans', cursive;"><i class="music icon"></i>Today's Song</button>
                <button onclick="start('startClick')" id="nextButton" class="ui violet labeled icon button" style="font-family: 'Balsamiq Sans', cursive;"><i class="arrow circle right icon"></i>Next</button>
            </div>
        </div>



     <div class="controlingArea">
            <h4 class="ui horizontal divider inverted header" style="font-family: 'Balsamiq Sans', cursive;"><i class="music icon"></i>&nbsp;&nbsp;Online&nbsp;&nbsp;<i class="music icon"></i></h4>
            <div class="controlButtons">

                <div class="scrolling content" style="color: black;">
                <table class="ui selectable inverted table">
                    <thead>
                        <tr>
                            <th class="center aligned">ID</th>
                            <th class="center aligned">Name</th>
                            <th class="center aligned">UID</th>
                            <th class="center aligned">Ban</th>
                             <th class="center aligned">IP</th>
                              <th class="center aligned">Location</th>
                      </tr>
                    </thead>
                    <tbody id="SongsListArea">

                    </tbody>
                  </table>
            </div>

                 </div>
        </div>
      


      
     <div class="controlingArea">
            <h4 class="ui horizontal divider inverted header" style="font-family: 'Balsamiq Sans', cursive;"><i class="music icon"></i>&nbsp;&nbsp;LOGGER&nbsp;&nbsp;<i class="music icon"></i></h4>
            <div class="controlButtons">
           <textarea id="output" cols="100" rows="35"></textarea>
                         <textarea id="output1" cols="100" rows="35"></textarea>
                                       <textarea id="output1" cols="100" rows="35"></textarea>


           </div>
        </div>


        <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/semantic-ui@2.4.2/dist/semantic.min.js"></script>
        <script type="text/javascript">

          function start(params) {
           switch (params) {
             case "/api/new-quest":
                           axios.get(params)
    .then(function (res) {
        console.log(res.data);
    })
    .catch(function (error) {});

             break;
             case "/api/run-quiz":
                  axios.get(params)
    .then(function (res) {
              document.getElementById('output').innerHTML = JSON.stringify(res.data, null, 4);

        console.log(res.data);
    })
    .catch(function (error) {});
                                  

            break;
                case "/api/send-answer":
                                              axios.get(params)
    .then(function (res) {
        console.log(res.data);
    })
    .catch(function (error) {});

            break;
                case "/api/send-Rewards":
                                               axios.get(params)
    .then(function (res) {
        console.log(res.data);
    })
    .catch(function (error) {});

            break;
                   case "/api/shut-down":
                                                axios.get(params)
    .then(function (res) {
        console.log(res.data);
    })
    .catch(function (error) {});

            break;
            default:
            console.log("not found: "+ params)
           }
           
          
          }
          
          
        </script>
          <script>

const socket = io();

  socket.on('api', (data) => {
          var str = "";
    jQuery.each(data.adminUsers, function( i, val ) {
      if (val.banned){
                 str+= "<tr style='background-color: #d37412;' ><td class='center aligned'>"+(i)+"</td><td class='center aligned'>"+val.username+"</td><td class='center aligned'>"+val.userId+"</td><td class='center aligned'>"+val.banned+"</td><td class='center aligned'>"+val.ip+"</td><td class='center aligned'>"+val.remark+"</td></tr>"
      }else{
          str+= "<tr><td class='center aligned'>"+(i)+"</td><td class='center aligned'>"+val.username+"</td><td class='center aligned'>"+val.userId+"</td><td class='center aligned'>"+val.banned+"</td><td class='center aligned'>"+val.ip+"</td><td class='center aligned'>"+val.remark+"</td></tr>"
      }
                  
});
      $("#SongsListArea").html(str)
        
    });
        
      </script>
    </body>
</html>